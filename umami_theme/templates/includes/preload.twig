{#
/**
 * @file
 * Preload the commonly used fonts for the headings and the body copy.
 *
 * Available variables:
 * - umami_theme_path: Returns the path to the UmamiTheme theme.
 */
#}

{#
  Keep these in sync with the vendored fonts under `umami_theme/fonts/`.
#}
<link rel="preload" href="{{ umami_theme_path }}/fonts/source-sans-pro-v21-latin-regular.woff2" as="font" type="font/woff2" crossorigin />
<link rel="preload" href="{{ umami_theme_path }}/fonts/scope-one-v14-latin-regular.woff2" as="font" type="font/woff2" crossorigin />
